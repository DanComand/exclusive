<%= form_for(@listing) do |f| %>

<div class="field">
	<%= f.label :address %><br />
	<%= f.text_field :address %>
</div>
<div class="field">
	<%= f.label :city %><br />
	<%= f.text_field :city %>
</div>
<div class="field">
	<%= f.label :bedrooms %><br />
	<%= f.number_field :bedrooms %>
</div>

<div class="field">
	<%= f.label :bathrooms %><br />
	<%= f.number_field :bathrooms %>
</div>

<div class="field">
	<%= f.label :squarefeet %><br />
	<%= f.number_field :squarefeet %>
</div>

<div class="field">
	<%= f.label :description %><br />
	<%= f.text_area :description %>
</div> 

<div class="actions">
	<%= f.submit %>
</div>

<% end %>